# AIOpsLab Environment Variables Configuration
# Copy this file to .env and replace placeholder values with your credentials.

# =============================================================================
# GENERAL SETTINGS
# =============================================================================

# Enable Weights & Biases logging (optional)
USE_WANDB=false

# Session variant support (toggle if you want orchestrator to use variants)
AIOPSLAB_USE_PROBLEM_VARIANTS=true

# =============================================================================
# API KEYS (replace the placeholders below)
# =============================================================================

# OpenAI API Key (for direct OpenAI usage if required)
OPENAI_API_KEY=your_openai_api_key_here

# OpenRouter API Key (primary key used by the OpenRouter client)
OPENROUTER_API_KEY=your_openrouter_api_key_here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MODEL=openai/gpt-4o-mini

# DeepSeek API Key (optional; uncomment when integrating DeepSeek client)
# DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Groq API Key (optional; uncomment if using Groq-backed models)
# GROQ_API_KEY=your_groq_api_key_here

# Alibaba DashScope API Key (for Qwen models; optional backup)
# DASHSCOPE_API_KEY=your_dashscope_api_key_here

# =============================================================================
# MODEL CONFIGURATION
# =============================================================================

# Default LLM model selection for local tooling/tests
MODEL=gpt-4o-mini-2024-07-18

# Uncomment to switch to a Qwen fallback model when needed
# MODEL=qwen2.5-coder-14b-instruct

# Supervisor model for secondary evaluations (optional)
SUPERVISOR_MODEL=google/gemini-2.5-pro

# =============================================================================
# BACKUP DASHCOPE SETTINGS (OPTIONAL)
# =============================================================================

# API_SOURCE=dashscope
# API_KEY=replace_with_dashscope_api_key
# API_BASE=https://dashscope.aliyuncs.com/compatible-mode/v1

